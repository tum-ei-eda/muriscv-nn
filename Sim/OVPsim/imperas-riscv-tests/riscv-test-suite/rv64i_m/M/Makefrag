# Description: Makefrag for M compliance tests

m_sc_tests = \
	MUL-01 \
	MULH-01 \
	MULHSU-01 \
	MULHU-01 \
	DIV-01 \
	DIVU-01 \
	REM-01 \
	REMU-01 \
	MULW-01 \
	DIVW-01 \
	DIVUW-01 \
	REMW-01 \
	REMUW-01 \

m_tests = $(addsuffix .elf, $(m_sc_tests))

target_tests += $(m_tests)

