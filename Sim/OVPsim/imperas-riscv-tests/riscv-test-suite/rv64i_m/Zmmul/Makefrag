# Description: Makefrag for Zmmul compliance tests

zmmul_sc_tests = \
	MUL-01 \
	MULH-01 \
	MULHSU-01 \
	MULHU-01 \
	MULW-01 \

zmmul_tests = $(addsuffix .elf, $(zmmul_sc_tests))

target_tests += $(zmmul_tests)

